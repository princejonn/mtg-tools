{
  "name": "mtg-tools",
  "version": "0.2.3",
  "main": "lib/index",
  "author": "jonn@jonn.nu",
  "description": "tools to help build MTG decks",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:princejonn/mtg-tools.git"
  },
  "bin": {
    "mtg-tools": "./lib/index.js"
  },
  "scripts": {
    "build": "cross-env BABEL_ENV=production babel src --out-dir lib",
    "check": "npm-check --update",
    "clean": "rimraf lib",
    "clean:modules": "npm run clean && rimraf package-lock.json && rimraf node_modules",
    "clean:install": "npm run clean:modules && npm install && git add package-lock.json",
    "lint": "eslint --ext .js ./src/",
    "lint:fix": "eslint --ext .js ./src/ --fix",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run clean && npm run lint && npm run build",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish",
    "test": "npm run build && npm i -g ./"
  },
  "dependencies": {
    "commander": "^2.20.0",
    "crypto-js": "^3.1.9-1",
    "csvtojson": "^2.0.10",
    "date-fns": "^1.30.1",
    "date-fns-timezone": "^0.1.4",
    "download": "^7.1.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.4.1",
    "inquirer-checkbox-plus-prompt": "^1.0.1",
    "lodash": "^4.17.11",
    "lodash-humps": "^3.1.2",
    "n-readlines": "^1.0.0",
    "nedb": "^1.8.0",
    "ora": "^3.4.0",
    "puppeteer": "^1.14.0",
    "query-string": "^6.4.2",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.17.3",
    "husky": "^3.0.0",
    "npm-check": "^5.9.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  }
}
