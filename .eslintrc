{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "airbnb-base"
  ],
  "rules": {
    "array-bracket-spacing": [ "error", "always" ],
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "class-methods-use-this": 0,
    "comma-dangle": [ "error", "always-multiline" ],
    "consistent-return": 0,
    "indent": [ "error", 2 ],
    "max-len": 0,
    "new-parens": 0,
    "no-await-in-loop": 0,
    "no-continue": 0,
    "no-irregular-whitespace": 1,
    "no-param-reassign": 0,
    "no-plusplus": 1,
    "no-prototype-builtins": 0,
    "no-restricted-syntax": 0,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "no-useless-escape": 1,
    "no-void": 0,
    "object-curly-newline": [ "error", { "multiline": true } ],
    "operator-linebreak": [ "error", "after", {
      "overrides": {
        "?": "before",
        ":": "before"
      }
    }],
    "radix": 0,
    "quotes": [ "error", "double" ]
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
